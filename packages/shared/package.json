{
  "name": "@side-ide/shared",
  "version": "1.0.0",
  "description": "Shared types and utilities for S-IDE",
  "type": "module",
  "main": "./dist/utils.js",
  "types": "./dist/utils.d.ts",
  "exports": {
    "./types": {
      "types": "./dist/types.d.ts",
      "default": "./dist/types.js"
    },
    "./utils": {
      "types": "./dist/utils.d.ts",
      "default": "./dist/utils.js"
    },
    "./utils-node": {
      "types": "./dist/utils-node.d.ts",
      "default": "./dist/utils-node.js"
    }
  },
  "files": [
    "dist",
    "*.ts"
  ],
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "typescript": "^5.7.2"
  }
}
